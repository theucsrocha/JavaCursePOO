package curso_programacao;


import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int y = sc.nextInt();
		
		if(x > y) {
			System.out.println(x - (24 - y));
		}
		else if(y > x) {
			System.out.println(y - (24 - x));
			
		}
		else {
			System.out.println(24);
		}
	}
}
